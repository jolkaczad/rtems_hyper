HeapSize = DEFINED(HeapSize) ? HeapSize : 
          DEFINED(_HeapSize) ? _HeapSize : 0x0;
RamBase = DEFINED(_RamBase) ? RamBase : 0x0;
RamSize = DEFINED(RamSize) ? RamSize    : 
          DEFINED(_RamSize) ? _RamSize : 0xFFFFFFFF;

ENTRY(bsp_start)
SECTIONS
{
	. = 0x0;

	__partition_begin = . ;

	.text :
	{
		*(.text)
	}

	.rodata :
	{
		*(.rodata)
	}

	.data :
	{
		*(.data) *(.bss) *(COMMON)
	}
	WorkAreaBase = .;
	__partition_end = . ;
} 
